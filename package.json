{
  "name": "timetable-sa",
  "version": "2.0.0",
  "description": "Generic, unopinionated Simulated Annealing library for constraint satisfaction problems. Solve timetabling, scheduling, resource allocation, and any optimization problem with custom constraints.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "example:basic": "tsx src/examples/basic-usage.ts",
    "example:custom": "tsx src/examples/custom-config.ts",
    "example:json": "tsx src/examples/json-usage.ts",
    "example:pdf": "tsx src/examples/generate-pdfs.ts",
    "dev": "tsx src/index-old.ts src/data_uisi.xlsx"
  },
  "keywords": [
    "simulated-annealing",
    "optimization",
    "constraint-satisfaction",
    "csp",
    "scheduling",
    "timetabling",
    "generic",
    "unopinionated",
    "typescript",
    "resource-allocation",
    "optimization-algorithm",
    "metaheuristic"
  ],
  "author": "Albert A Bayor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/albertabayor/simulated-annealing-university-timetabling-course-problem.git"
  },
  "bugs": {
    "url": "https://github.com/albertabayor/simulated-annealing-university-timetabling-course-problem/issues"
  },
  "homepage": "https://github.com/albertabayor/simulated-annealing-university-timetabling-course-problem#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/pdfkit": "^0.17.3",
    "pdfkit": "^0.17.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
